{
  "version": 2,
  "name": "ecologico",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "functions": {
    "frontend/pages/api/*.js": {
      "runtime": "nodejs18.x"
    }
  }
} 